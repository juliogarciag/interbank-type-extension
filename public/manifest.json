{
  "manifest_version": 3,
  "name": "interbank-type",
  "version": "1.0",
  "description": "",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["scripting", "tabs", "activeTab"],
  "host_permissions": ["https://*/*", "http://*/*"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/content-script.ts"],
      "run_at": "document_end"
    }
  ]
}
